@model EFDbFirstApproachExample.Identity.ApplicationUser
@{
    ViewBag.Title = "MyProfile";
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
}

<h2>MyProfile</h2>

<table>
    <tr>
        <th> UserName </th>
        <td> @Model.UserName</td>
    </tr>
    <tr>
        <th> Email </th>
        <td> @Model.Email</td>
    </tr>
    <tr>
        <th> Mobile </th>
        <td> @Model.PhoneNumber</td>
    </tr>
    <tr>
        <th> Birthday </th>
        <td> @Model.Birthday</td>
    </tr>
    <tr>
        <th> Address </th>
        <td> @Model.Address</td>
    </tr>
    <tr>
        <th> City </th>
        <td> @Model.City</td>
    </tr>
    <tr>
        <th> Country </th>
        <td> @Model.Country</td>
    </tr>
</table>
<a href="/home/index"> Back </a>